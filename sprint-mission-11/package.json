{
  "scripts": {
    "start": "node ./build/main.js",
    "build": "tsc",
    "dev": "nodemon ./src/main.ts --watch ./src",
    "test": "jest",
    "db:test": "dotenv -e .env.test -- prisma migrate deploy --schema prisma/schema.prisma",
    "test:it": "dotenv -e .env.test -- jest --runInBand",

    "format:check": "prettier . --check",
    "type:check": "tsc --noEmit",
    "test:unit": "export NODE_ENV=test && jest src/test/unit --runInBand",
    "test:e2e": "export NODE_ENV=test && jest src/test/e2e --runInBand"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/multer": "^1.4.12",
    "@types/multer-s3": "^3.0.3",
    "@types/supertest": "^6.0.3",
    "dotenv-cli": "^11.0.0",
    "jest": "^30.2.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.3.2",
    "prisma": "^5.16.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.958.0",
    "@prisma/client": "^5.16.2",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "socket.io": "^4.8.1",
    "superstruct": "^2.0.2",
    "uuid": "^11.0.5"
  }
}
